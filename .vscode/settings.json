{
    "cSpell.language": "en,de",
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.rulers": [
        80,
        120
    ],
    "cSpell.words": [
        "dhbw",
        "DHBW",
        "engineio",
        "github",
        "googollee",
        "mattn",
        "openapi",
        "redis",
        "Richert",
        "sqlite",
        "yannickkirschen"
    ],
    "python.formatting.provider": "none",
    "python.formatting.autopep8Args": [
        "--max-line-length=140"
    ],
    "python.formatting.blackArgs": [
        "--line-length=140"
    ],
    "python.formatting.yapfArgs": [
        "--style={based_on_style: google, column_limit: 140, indent_width: 4}"
    ]
}